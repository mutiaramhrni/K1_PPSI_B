<div class="wrapper-buat-kelas w-100">
      <form action="/buat-kelas" class="form-buat-kelas" method="post">
          <h5>Buat kelas</h5>
            <div class="my-3">
                <input type="text" class="title fw-bold" name="title" placeholder="Nama kelas" required>
              </div>
            <div class="mb-3">
              <input type="text" class="title" name="enroll_key" placeholder="Enroll key" required>
            </div>
            <div class="mb-3">
              <input type="text" class="title" name="excerpt" placeholder="Nama proyek" required>
            </div>
          <div class="mt-3 con-btn">
            <a href="/kelas" class="btn-cancel">Cancel</a>
            <button type="submit" class="btn-publish">Publish</button>
          </div>
        </form>
  </div>
  