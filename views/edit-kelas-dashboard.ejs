<div class="wrapper-buat-kelas">
  <form action="/edit-kelas-dashboard" class="form-buat-kelas" method="post">
      <h5>Edit Kelas</h5>
      <div class="my-3">
        <input type="hidden" class="title fw-bold" name="id_kelas" placeholder="Nama kelas" value="<%= kelas.id_kelas %>">
      </div>
      <div class="my-3">
        <input type="text" class="title fw-bold" name="title" placeholder="Nama kelas" value="<%= kelas.title %>">
      </div>
    <div class="mb-3">
      <input type="text" class="title" name="enroll_key" placeholder="Enroll key" value="<%= kelas.enroll_key %>">
    </div>
    <div class="mb-3">
      <input type="text" class="title" name="excerpt" placeholder="Nama proyek" value="<%= kelas.excerpt %>">
    </div>
      <div class="mt-3 con-btn">
        <a href="/kelola-kelas" class="btn-cancel">Cancel</a>
        <button type="submit" class="btn-publish">Simpan</button>
      </div>
    </form>
</div>